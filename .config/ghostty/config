term = xterm-256color
theme = duskfox

font-family = SF Mono
font-family = "Hiragino Kaku Gothic ProN"
font-size = 12

font-feature = -calt
font-feature = -dlig
font-feature = -liga
keybind = shift+enter=text:\n
font-thicken = true

# For switching tabs on Mac with JIS Keyboard
keybind = super+shift+bracket_right=unbind
keybind = super+shift+[=previous_tab
keybind = super+]=goto_split:next

macos-titlebar-style = tabs

window-width = 164
window-height = 54
